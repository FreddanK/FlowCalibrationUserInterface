<Page x:Class="FlowCalibration.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FlowCalibration"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Page1">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Content="Sine" Name="Sine_Button" Click="Sine_Button_Click"/>
        <Button Grid.Column="1" Name="Square_Button" Content="Square" Click="Square_Button_Click"/>
        <Button Grid.Column="2" Name="Ramp_Button" Content="Ramp" Click="Ramp_Button_Click"/>
        <Button Grid.Row="1" Name="Triangle_Button" Content="Triangle" Click="Triangle_Button_Click"/>
        <Button Grid.Row="1" Grid.Column="1" Name="Peaks_Button" Content="Inhale-Exhale" Click="Peaks_Button_Click"/>
        <Button Grid.Row="1" Grid.Column="2" Name="Custom_Button" Content="Custom" Click="Custom_Button_Click"/>
        
    </Grid>
</Page>
