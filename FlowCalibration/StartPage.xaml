<Page x:Class="FlowCalibration.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FlowCalibration"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Page1">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Content="Sine" Click="StartPageButton_Click"/>
        <Button Grid.Column="1" Content="Square" Click="StartPageButton_Click"/>
        <Button Grid.Column="2" Content="Ramp" Click="StartPageButton_Click"/>
        <Button Grid.Row="1" Content="Triangle" Click="StartPageButton_Click"/>
        <Button Grid.Row="1" Grid.Column="1" Content="Inhale-Exhale" Click="StartPageButton_Click"/>
        <Button Grid.Row="1" Grid.Column="2" Content="Custom" Click="StartPageButton_Click"/>
        
    </Grid>
</Page>
